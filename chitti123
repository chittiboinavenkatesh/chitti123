<h2>JavaScript Object Constructors</h2>

<p id="demo"></p>

<script>

function Person(first, last, age, id) {
  this.firstName = first;
  this.lastName = last;
  this.age = age;
  this.id = id;
}
 const country={name:'suresh',coun:function(name,age){
  document.write("hello"+"<br>");
  this.name=name;
            this.age=age;
            document.write(name+'<br>'); 
            document.write(age+'<br>');
            
        }}
        
         country.coun('suresh',25); 
const test= new Person("suresh","kumar",24, 897);
document.getElementById("demo").innerHTML = "my age is" +test.age  + " "; 

</script>
